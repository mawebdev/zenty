    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
